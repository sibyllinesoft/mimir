{
  "dashboard": {
    "id": null,
    "title": "Mimir Pipeline Monitoring",
    "description": "Comprehensive monitoring of the 6-stage repository indexing pipeline",
    "tags": ["mimir", "pipeline", "indexing"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Pipeline Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(mimir_pipeline_executions_total{status=\"success\"}[5m]) / rate(mimir_pipeline_executions_total[5m]) * 100",
            "legendFormat": "Success Rate %",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          }
        ],
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 95}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Active Pipelines",
        "type": "stat",
        "targets": [
          {
            "expr": "mimir_pipeline_active_count",
            "legendFormat": "Active",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          }
        ],
        "gridPos": {"x": 6, "y": 0, "w": 3, "h": 4},
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Pipeline Queue",
        "type": "stat",
        "targets": [
          {
            "expr": "mimir_pipeline_queue_length",
            "legendFormat": "Queued",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          }
        ],
        "gridPos": {"x": 9, "y": 0, "w": 3, "h": 4},
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 3},
                {"color": "red", "value": 8}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Pipeline Duration by Stage",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(mimir_pipeline_duration_seconds_bucket{stage!=\"\"}[5m]))",
            "legendFormat": "P95 - {{stage}}",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          },
          {
            "expr": "histogram_quantile(0.50, rate(mimir_pipeline_duration_seconds_bucket{stage!=\"\"}[5m]))",
            "legendFormat": "P50 - {{stage}}",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          }
        ],
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"},
          "tooltip": {"mode": "multi", "sort": "desc"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {"drawStyle": "line", "lineInterpolation": "linear"}
          }
        }
      },
      {
        "id": 5,
        "title": "Stage Success Rates",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mimir_pipeline_executions_total{status=\"success\",stage!=\"\"}[5m]) / rate(mimir_pipeline_executions_total{stage!=\"\"}[5m]) * 100",
            "legendFormat": "{{stage}}",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          }
        ],
        "gridPos": {"x": 0, "y": 12, "w": 6, "h": 8},
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "custom": {"drawStyle": "line", "lineInterpolation": "linear"}
          }
        }
      },
      {
        "id": 6,
        "title": "Files Processed per Stage",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(mimir_pipeline_files_processed_bucket{stage!=\"\"}[5m]))",
            "legendFormat": "P95 - {{stage}}",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          },
          {
            "expr": "histogram_quantile(0.50, rate(mimir_pipeline_files_processed_bucket{stage!=\"\"}[5m]))",
            "legendFormat": "P50 - {{stage}}",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          }
        ],
        "gridPos": {"x": 6, "y": 12, "w": 6, "h": 8},
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {"drawStyle": "line", "lineInterpolation": "linear"}
          }
        }
      },
      {
        "id": 7,
        "title": "Error Rate by Stage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mimir_pipeline_errors_total{stage!=\"\"}[5m])",
            "legendFormat": "{{stage}} - {{error_type}}",
            "datasource": {"type": "prometheus", "uid": "prometheus"}
          }
        ],
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {"drawStyle": "line", "lineInterpolation": "linear"}
          }
        }
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "timepicker": {},
    "templating": {"list": []},
    "annotations": {"list": []},
    "refresh": "30s",
    "schemaVersion": 30,
    "style": "dark",
    "version": 1,
    "weekStart": ""
  },
  "overwrite": true
}